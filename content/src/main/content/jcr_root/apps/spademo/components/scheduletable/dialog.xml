<?xml version="1.0" encoding="UTF-8"?>
<jcr:root 
  title="Schedule Table Component"
  xtype="panel"
  helpPath="en/cq/current/wcm/default_components.html#Title"
  jcr:primaryType="cq:Dialog"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:cq="http://www.day.com/jcr/cq/1.0">
  <items jcr:primaryType="cq:WidgetCollection">
    <title 
      xtype="textfield"
      jcr:primaryType="cq:Widget"
      name="./jcr:title"
      fieldLabel="Title"
      allowBlank="false"
      defaultValue="adaptTo 2014 Schedule"/>
    <headerTime 
      xtype="textfield"
      jcr:primaryType="cq:Widget"
      name="./headerTime"
      fieldLabel="Header-Time"
      allowBlank="false"
      defaultValue="Time"/>
    <headerTalk 
      xtype="textfield"
      jcr:primaryType="cq:Widget"
      name="./headerTalk"
      fieldLabel="Header-Talk"
      allowBlank="false"
      defaultValue="Talk"/>
    <headerSpeakers 
      xtype="textfield"
      jcr:primaryType="cq:Widget"
      name="./headerSpeakers"
      fieldLabel="Header-Speakers"
      allowBlank="false"
      defaultValue="Speakers"/>
  </items>
</jcr:root>